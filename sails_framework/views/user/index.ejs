<div class="container">
	<h3>Users</h3>
	<table class="table">
		<tr>
			<th></th>
			<th>ID</th>
			<th>Name</th>
			<th>Title</th>
			<th>Email</th>
			<th></th>
			<th></th>
			<th></th>
		</tr>
	<% _.each(users, function(user){ %>
	<tr data-id="<%= user.id %>" data-model="user">
		<td>
			<img id="onlinetag"
				<% if(user.online){ %> 
					src="/images/online.png" 
				 <% }else{ %>
				 	src="/images/offline.png"
				 <% } %>
			width="40" height="40">
		</td>
		<td><%= user.id %></td>
		<td><%= user.name %></td>
		<td><%= user.title %></td>
		<td><%= user.email %></td>
		<% if(user.admin){ %>
			<td><img src="/images/admin.png" width="40" height="40"></td>
			<% }else{ %>
			<td><img src="/images/user.png"  width="40" height="40"></td>
		<% } %>
		<td><a href="/user/show/<%= user.id %>" class="btn btn-sm btn-primary">show</a></td> 
		<td><a href="/user/edit/<%= user.id %>" class="btn btn-sm btn-warning">Edit</a></td>
		<td><form action="/user/destroy/<%= user.id %>" method="POST">
		<input type="hidden" name="method" value="delete">
		<input type="submit" class="btn btn-sm btn-danger" value="Delete" >
		<input type="hidden" name="_csrf" value="<%= _csrf %>">
		</form></td>
	</tr>

	<% }) %>

	</table>
	<ul id="user-list">
</ul>

</div>
